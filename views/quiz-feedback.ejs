<!-- quiz-feedback.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quiz Feedback</title>
    <link rel="stylesheet" href="styles/main3.css">
</head>
<body>
    <h1>Quiz Feedback</h1>
    <p>Your score: <%= score %> / <%= totalQuestions %></p>
    <p><%= score === totalQuestions ? "Congratulations! You got all the questions correct!" : "Better luck next time!" %></p>
    <p><a href="/player">Back to Home</a></p>
</body>
</html>
